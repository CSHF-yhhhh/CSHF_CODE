<FinishPage>:
    primary_color: app.theme_cls.primary_dark
    MDBoxLayout:
        orientation: "vertical"
        size_hint: (1, 1)
        spacing:10
        padding: 10
        canvas:
            Color:
                rgba: (0.95, 0.95, 0.95, 1)
            Rectangle:
                size: self.size
                pos: self.pos
                source: "appPicture/back3.jpg"
        MDBoxLayout:
            size_hint: (1, 0.1)
            MDIconButton:
                icon: "backspace-reverse-outline"
                pos_hint: {"center_y" : 0.5}
                on_release: app.ColseFinishPage()
        MDBoxLayout:
            size_hint: (1, 0.1)
            canvas:
                Color:
                    rgba: (1, 1, 1, 1)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    source: "appPicture/paper.jpg"
            MDLabel:
                text: "本次学习内容"
                font_style: "H6"
                halign: "center"
        ScrollView:
            size_hint: (1, 0.9)
            MDGridLayout:
                size_hint: (1, None)
                id: boxs
                adaptive_height: True
                cols: 1
                spacing: 5
                padding: 5
<Item>:
    on_release: app.ShowDetail(self.content)
    canvas.before:
        Color:
            rgba: (1,1,1,1)
        RoundedRectangle:
            size:self.size
            pos: self.pos
            source: "./appPicture/paper.jpg"
    IconLeftWidget:
        icon: "bookmark-outline"
        pos_hint: {"center_y":0.5}
        
    
    